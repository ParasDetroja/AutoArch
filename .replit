modules = ["cpp"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Unreal Engine Plugin"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "unreal_engine_test"

[[workflows.workflow]]
name = "Unreal Engine Plugin"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = '''echo "Unreal Engine FloorPlanGenerator Plugin Setup" && echo "Plugin structure created successfully!" && echo "Plugin files:" && find . -name "*.uplugin" -o -name "*.h" -o -name "*.cpp" -o -name "*.cs" -o -name "*.uasset" | head -20 && echo "" && echo "Floor plan input image available at: attached_assets/00_input_1755259407356.png" && echo "" && echo "Plugin components created:" && echo "✓ FloorPlanGenerator.uplugin - Main plugin descriptor" && echo "✓ Source/FloorPlanGenerator/ - C++ source code" && echo "✓ Content/Blueprints/ - Blueprint assets" && echo "✓ Content/Materials/ - Material assets" && echo "" && echo "To use this plugin:" && echo "1. Copy the entire project to your Unreal Engine Plugins folder" && echo "2. Enable the FloorPlanGenerator plugin in Unreal Engine" && echo "3. Use BP_FloorPlanGenerator blueprint to process floor plans" && echo "4. Configure wall, window, and door heights as needed" && python3 -c "print('\nPlugin ready for Unreal Engine integration!')" && sleep infinity'''

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "unreal_engine_test"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd /home/runner/FloorPlanGenerator && /opt/UnrealEngine/Engine/Binaries/Linux/UnrealEditor ."
